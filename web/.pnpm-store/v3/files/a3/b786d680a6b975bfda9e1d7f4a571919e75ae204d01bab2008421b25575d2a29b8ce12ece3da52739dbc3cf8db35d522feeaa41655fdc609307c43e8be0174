import './chunk-BYXBJQAS.js';
import { __toESM, require_picocolors } from './chunk-3VVDG2YT.js';
import { lt } from 'semver';
import { dedent } from 'ts-dedent';

var import_picocolors=__toESM(require_picocolors(),1);var blocker={id:"minimumNode16",async check(){let nodeVersion=process.versions.node;return nodeVersion&&lt(nodeVersion,"18.0.0")?{nodeVersion}:!1},log(options,data){return dedent`
      We've detected you're using Node.js v${data.nodeVersion}.
      Storybook needs Node.js 18 or higher.

      ${import_picocolors.default.yellow("https://nodejs.org/en/download")}
    `}};

export { blocker };
